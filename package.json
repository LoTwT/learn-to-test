{
  "name": "learn-to-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.26.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier -w .",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "lint-staged": "^13.1.0",
    "pnpm": "^7.26.2",
    "prettier": "^2.8.3",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.8.5",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier -w --ignore-unknown"
    ]
  }
}
