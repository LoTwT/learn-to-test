{
  "name": "learn-to-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.26.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier -w .",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/types": "^29.4.1",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "babel-jest": "^29.4.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.0",
    "pnpm": "^7.26.2",
    "prettier": "^2.8.3",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.8.5",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier -w --ignore-unknown"
    ]
  }
}
